{
	"format_version": "1.13.0",
	"minecraft:entity": {
		"description": {
			"identifier": "minecraft:tnt_minecart",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false
		},
		"component_groups": {
			"minecraft:primed_tnt": {
				"minecraft:explode": {
					"fuse_length": 4,
					"fuse_lit": true,
					"power": 3,
					"causes_fire": false
				},
				"minecraft:rail_sensor": {},
				"minecraft:is_ignited": {}
			},
			"minecraft:instant_explode_tnt": {
				"minecraft:explode": {
					"fuse_length": 0,
					"fuse_lit": true,
					"power": 3,
					"causes_fire": false
				},
				"minecraft:rail_sensor": {},
				"minecraft:is_ignited": {}
			},
			"minecraft:inactive": {
				"minecraft:rail_sensor": {
					"on_activate": {
						"filters": {
							"all_of": [
								{
									"test": "is_game_rule",
									"domain": "tntexplodes",
									"operator": "==",
									"value": true
								}
							]
						},
						"event": "minecraft:on_prime"
					}
				},
				"minecraft:interact": {
					"interactions": [
						{
							"on_interact": {
								"filters": {
									"any_of": [
										{
											"test": "has_equipment",
											"subject": "other",
											"domain": "hand",
											"value": "fireball:0"
										},
										{
											"test": "has_equipment",
											"subject": "other",
											"domain": "hand",
											"value": "flint_and_steel"
										}
									],
									"all_of": [
										{
											"test": "is_family",
											"subject": "other",
											"value": "player"
										},
										{
											"test": "is_game_rule",
											"domain": "tntexplodes",
											"operator": "==",
											"value": true
										}
									]
								},
								"event": "minecraft:on_prime",
								"target": "self"
							},
							"swing": true,
							"play_sounds": "ignite",
							"interact_text": "action.interact.creeper"
						},
						{
							"on_interact": {
								"filters": {
									"any_of": [
										{
											"test": "has_component",
											"subject": "other",
											"value": "fire_aspect"
										}
									],
									"all_of": [
										{
											"test": "is_game_rule",
											"domain": "tntexplodes",
											"operator": "==",
											"value": true
										}
									]
								},
								"event": "minecraft:on_prime",
								"target": "self"
							},
							"swing": true,
							"interact_text": "action.interact.creeper"
						},
						{
							"on_interact": {
								"event": "minecraft:speed_menu",
								"target": "self"
							},
							"condition": "query.is_sneaking",
							"use_item": false
						}
					]
				}
			},
			"minecraft:slow_speed": {
				"minecraft:rail_movement": {
					"max_speed": 0.4
				}
			},
			"minecraft:normal_speed": {
				"minecraft:rail_movement": {
					"max_speed": 1.0
				}
			},
			"minecraft:fast_speed": {
				"minecraft:rail_movement": {
					"max_speed": 2.0
				}
			},
			"minecraft:very_fast_speed": {
				"minecraft:rail_movement": {
					"max_speed": 5.0
				}
			},
			"minecraft:super_fast_speed": {
				"minecraft:rail_movement": {
					"max_speed": 10.0
				}
			}
		},
		"components": {
			"minecraft:type_family": {
				"family": [
					"minecart",
					"inanimate"
				]
			},
			"minecraft:collision_box": {
				"width": 0.98,
				"height": 0.7
			},
			"minecraft:rail_movement": {
				"max_speed": 1
			},
			"minecraft:is_stackable": {},
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": true,
				"is_pushable_by_piston": true
			},
			"minecraft:conditional_bandwidth_optimization": {
				"default_values": {
					"max_optimized_distance": 0,
					"max_dropped_ticks": 0,
					"use_motion_prediction_hints": true
				},
				"conditional_values": [
					{
						"max_optimized_distance": 0,
						"max_dropped_ticks": 0,
						"conditional_values": [
							{
								"test": "is_moving",
								"subject": "self",
								"operator": "==",
								"value": true
							}
						]
					}
				]
			}
		},
		"events": {
			"minecraft:entity_spawned": {
				"add": {
					"component_groups": [
						"minecraft:inactive",
						"minecraft:normal_speed"
					]
				}
			},
			"minecraft:on_prime": {
				"remove": {
					"component_groups": [
						"minecraft:inactive"
					]
				},
				"add": {
					"component_groups": [
						"minecraft:primed_tnt"
					]
				}
			},
			"minecraft:on_instant_prime": {
				"remove": {
					"component_groups": [
						"minecraft:inactive"
					]
				},
				"add": {
					"component_groups": [
						"minecraft:instant_explode_tnt"
					]
				}
			},
			"minecraft:speed_menu": {
				"run_command": {
					"command": [
						"tellraw @p [\"\",{\"text\":\"TNT Minecart Speed Options:\\n\",\"color\":\"gold\"},{\"text\":\"0.4 - Slow (Vanilla)\\n\",\"color\":\"gray\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/execute @e[type=minecraft:tnt_minecart,c=1] ~~~ event minecraft:set_slow_speed\"}},{\"text\":\"1.0 - Normal (Current)\\n\",\"color\":\"white\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/execute @e[type=minecraft:tnt_minecart,c=1] ~~~ event minecraft:set_normal_speed\"}},{\"text\":\"2.0 - Fast\\n\",\"color\":\"green\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/execute @e[type=minecraft:tnt_minecart,c=1] ~~~ event minecraft:set_fast_speed\"}},{\"text\":\"5.0 - Very Fast\",\"color\":\"yellow\",\"clickEvent\":{\"action\":\"run_command\",\"value\":\"/execute @e[type=minecraft:tnt_minecart,c=1] ~~~ event minecraft:set_very_fast_speed\"}}]"
					]
				}
			},
			"minecraft:set_slow_speed": {
				"remove": {
					"component_groups": [
						"minecraft:normal_speed",
						"minecraft:fast_speed",
						"minecraft:very_fast_speed",
						"minecraft:super_fast_speed"
					]
				},
				"add": {
					"component_groups": [
						"minecraft:slow_speed"
					]
				},
				"run_command": {
					"command": [
						"tellraw @p [\"\",{\"text\":\"TNT Minecart speed set to \",\"color\":\"aqua\"},{\"text\":\"0.4\",\"color\":\"gray\",\"bold\":true},{\"text\":\" (Slow)\",\"color\":\"gray\"}]"
					]
				}
			},
			"minecraft:set_normal_speed": {
				"remove": {
					"component_groups": [
						"minecraft:slow_speed",
						"minecraft:fast_speed",
						"minecraft:very_fast_speed",
						"minecraft:super_fast_speed"
					]
				},
				"add": {
					"component_groups": [
						"minecraft:normal_speed"
					]
				},
				"run_command": {
					"command": [
						"tellraw @p [\"\",{\"text\":\"TNT Minecart speed set to \",\"color\":\"aqua\"},{\"text\":\"1.0\",\"color\":\"white\",\"bold\":true},{\"text\":\" (Normal)\",\"color\":\"white\"}]"
					]
				}
			},
			"minecraft:set_fast_speed": {
				"remove": {
					"component_groups": [
						"minecraft:slow_speed",
						"minecraft:normal_speed",
						"minecraft:very_fast_speed",
						"minecraft:super_fast_speed"
					]
				},
				"add": {
					"component_groups": [
						"minecraft:fast_speed"
					]
				},
				"run_command": {
					"command": [
						"tellraw @p [\"\",{\"text\":\"TNT Minecart speed set to \",\"color\":\"aqua\"},{\"text\":\"2.0\",\"color\":\"green\",\"bold\":true},{\"text\":\" (Fast)\",\"color\":\"green\"}]"
					]
				}
			},
			"minecraft:set_very_fast_speed": {
				"remove": {
					"component_groups": [
						"minecraft:slow_speed",
						"minecraft:normal_speed",
						"minecraft:fast_speed",
						"minecraft:super_fast_speed"
					]
				},
				"add": {
					"component_groups": [
						"minecraft:very_fast_speed"
					]
				},
				"run_command": {
					"command": [
						"tellraw @p [\"\",{\"text\":\"TNT Minecart speed set to \",\"color\":\"aqua\"},{\"text\":\"5.0\",\"color\":\"yellow\",\"bold\":true},{\"text\":\" (Very Fast)\",\"color\":\"yellow\"}]"
					]
				}
			},
			"minecraft:set_super_fast_speed": {
				"remove": {
					"component_groups": [
						"minecraft:slow_speed",
						"minecraft:normal_speed",
						"minecraft:fast_speed",
						"minecraft:very_fast_speed"
					]
				},
				"add": {
					"component_groups": [
						"minecraft:super_fast_speed"
					]
				},
				"run_command": {
					"command": [
						"tellraw @p [\"\",{\"text\":\"TNT Minecart speed set to \",\"color\":\"aqua\"},{\"text\":\"10.0\",\"color\":\"red\",\"bold\":true},{\"text\":\" (Super Fast)\",\"color\":\"red\"}]"
					]
				}
			}
		}
	}
}